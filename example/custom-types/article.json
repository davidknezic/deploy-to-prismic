{
  "@title": "Article",
  "@repeatable": true,
  "Main": {
    "uid": {
      "type": "UID",
      "config": {
        "label": "Slug",
        "placeholder": "ex. announcing-project-x"
      }
    },
    "title": {
      "type": "StructuredText",
      "config": {
        "useAsTitle": true,
        "single": "heading1",
        "label": "Main Title",
        "placeholder": "ex. Announcing Project X"
      }
    },
    "author": {
      "type": "Link",
      "config": {
        "select": "document",
        "customtypes": [
          "author"
        ],
        "label": "Author",
        "placeholder": "ex. John Doe"
      }
    }
  }
}
